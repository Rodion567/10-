import sqlite3 as sql
connection=sql.connect("main.sl3",5)
cur=connection.cursor()
cur.execute("DROP TABLE bank")
cur.execute("CREATE TABLE bank (id INT,first_name TEXT,last_name TEXT,money INT)")
cur.execute("INSERT INTO bank VALUES(1,'rodion','semibratov',123)")
cur.execute("INSERT INTO bank VALUES(1,'maks','tsjrh',33745)")
cur.execute("INSERT INTO bank VALUES(1,'anton','ardukov',7586098)")
cur.execute("INSERT INTO bank VALUES(1,'artem','albadukov',225)")
cur.execute("INSERT INTO bank VALUES(1,'bhbyf','serbardukova',8767)")
cur.execute('Select * from bank')
res=cur.fetchall()
print(res)
connection.commit()
connection.close()
